{
  "name": "@knocklabs/cli",
  "version": "0.1.0-rc.0",
  "description": "Knock CLI",
  "author": "@knocklabs",
  "bin": {
    "knock": "./bin/run"
  },
  "homepage": "https://github.com/knocklabs/knock-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "knocklabs/knock-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.26.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.3.0",
    "@prantlf/jsonlint": "^11.7.0",
    "axios": "^1.3.2",
    "date-fns": "^2.29.3",
    "enquirer": "^2.3.6",
    "fs-extra": "^10.1.0",
    "liquidjs": "^10.5.0",
    "lodash": "^4.17.21",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.6",
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.29",
    "@swc/helpers": "^0.4.14",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.12",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "mocha": "^9",
    "nock": "^13.3.0",
    "oclif": "^3",
    "prettier": "2.8.4",
    "shx": "^0.3.4",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "knock",
    "dirname": "knock",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "workflow": {
        "description": "interact with and manage workflows"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && swc src -d dist",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "lint.fix": "yarn run lint --fix",
    "format": "prettier \"src/**/*.+(ts|js)\" \"test/**/*.+(ts|js)\"",
    "format.write": "yarn run format --write",
    "format.check": "yarn run format --check",
    "type.check": "tsc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/knocklabs/knock-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
